  OUT.println(&apos;[PRODUCT] setting category for &apos; + category);
  row.categories[0] = allowedFunctions[mach[1]](mach[2]);
}

var base = row.base;
if (base) {
  var mach = base.match(/^(\w*[^ ])\((.*)\)$/);
  if (mach &amp;&amp; allowedFunctions[mach[1]]) {
    row.base = allowedFunctions[mach[1]](mach[2]);
  }
}

vars.putObject(&apos;dataToSend&apos;, JSON.stringify(row));</stringProp>
