}

function categoryIdByTitle(title) {
  return cats[title]
}

function productIdBySku(sku) {
  return prods[sku]
}

var allowedFunctions = {
  categoryIdByTitle: categoryIdByTitle,
  productIdBySku: productIdBySku
};

var row = JSON.parse(vars.get(&apos;row&apos;))

